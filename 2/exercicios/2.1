INSERTION-SORT-REVERSE(A)
for j <- comprimento(a) to 2
  do chave <- a[j]
    i <- j - 1
    while i < j e a[i] < chave
      do a[i+1] = a[i]
      i <- i + 1
    a[i+1] = chave

